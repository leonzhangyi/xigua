<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/include_toolbar" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10">

        <TextView
            android:id="@+id/agent_my_user_all"
            android:layout_width="@dimen/size_dp_50"
            android:layout_height="@dimen/size_dp_30"
            android:layout_marginLeft="@dimen/size_dp_16"
            android:background="@color/net_resource_item_tv"
            android:gravity="center"
            android:text="全部"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_14" />

        <RelativeLayout
            android:id="@+id/agent_user_start_time"
            android:layout_width="@dimen/size_dp_80"
            android:layout_height="@dimen/size_dp_30"
            android:layout_marginLeft="@dimen/size_dp_5"
            android:layout_toRightOf="@+id/agent_my_user_all"
            android:background="@drawable/layout_agent_my_user_time_bac">

            <TextView
                android:id="@+id/agent_user_start_time_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="2020/02/25"
                android:textColor="@color/black_D9"
                android:textSize="@dimen/size_sp_12" />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/agent_user_end_time"
            android:layout_width="@dimen/size_dp_80"
            android:layout_height="@dimen/size_dp_30"
            android:layout_marginLeft="@dimen/size_dp_5"
            android:layout_toRightOf="@+id/agent_user_start_time"
            android:background="@drawable/layout_agent_my_user_time_bac">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="2020/02/25"
                android:textColor="@color/black_D9"
                android:textSize="@dimen/size_sp_12" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout_agent_my_user_time_2"
            android:layout_width="@dimen/size_dp_80"
            android:layout_height="@dimen/size_dp_30"
            android:layout_marginLeft="@dimen/size_dp_5"
            android:layout_toRightOf="@+id/agent_user_start_time"
            android:background="@drawable/layout_agent_my_user_time_bac">

            <TextView
                android:id="@+id/agent_user_end_time_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="2020/02/25"
                android:textColor="@color/black_D9"
                android:textSize="@dimen/size_sp_12" />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/agent_user_search"
            android:layout_width="@dimen/size_dp_30"
            android:layout_height="@dimen/size_dp_30"
            android:layout_marginLeft="@dimen/size_dp_5"
            android:layout_toRightOf="@+id/layout_agent_my_user_time_2"
            android:background="@color/net_resource_item_tv">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:src="@mipmap/agent_use_search" />

        </RelativeLayout>

        <TextView
            android:id="@+id/agent_user_total"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_toRightOf="@+id/agent_user_search"
            android:text="总用户：100"
            android:textColor="@color/black_D9"
            android:textSize="@dimen/size_sp_10" />
    </RelativeLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_dp_30"
        android:layout_marginLeft="@dimen/size_dp_16"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginRight="@dimen/size_dp_16"
        android:layout_marginBottom="@dimen/dp_10">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="@dimen/dp_10"
            android:layout_toLeftOf="@+id/agent_user_one_search_rl"
            android:background="@drawable/layout_agent_my_user_time_bac">

            <EditText
                android:id="@+id/agent_use_et"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp_10"
                android:background="@null"
                android:hint="输入用户名"
                android:textColor="@color/black_D9"
                android:textColorHint="@color/black_40"
                android:textSize="@dimen/size_sp_12" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/agent_user_one_search_rl"
            android:layout_width="@dimen/size_dp_30"
            android:layout_height="@dimen/size_dp_30"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="@dimen/size_dp_5"
            android:background="@color/net_resource_item_tv">

            <ImageView
                android:id="@+id/agent_user_one_search"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:src="@mipmap/agent_use_search" />

        </RelativeLayout>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_dp_30"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.36"
            android:background="#ffe5e5e5"
            android:gravity="center"
            android:text="用户名"
            android:textColor="@color/black_D9"
            android:textSize="@dimen/size_sp_10" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#fff1f2f5"
            android:gravity="center"
            android:text="会员类型"
            android:textColor="@color/black_D9"
            android:textSize="@dimen/size_sp_10" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#ffe5e5e5"
            android:gravity="center"
            android:text="注册时间"
            android:textColor="@color/black_D9"
            android:textSize="@dimen/size_sp_10" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#fff1f2f5"
            android:gravity="center"
            android:text="到期时间"
            android:textColor="@color/black_D9"
            android:textSize="@dimen/size_sp_10" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#fff1f2f5"
            android:gravity="center"
            android:text="操作"
            android:textColor="@color/black_D9"
            android:textSize="@dimen/size_sp_10" />
    </LinearLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/agent_my_user_rec"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</LinearLayout>